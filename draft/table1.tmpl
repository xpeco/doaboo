<body onload="javascript:AplicarCebra('DataTable');">

<div class="ext font">

<!--Menu with scriptaculous-1.8.3 --->
<TMPL_INCLUDE NAME="menubar/menu.tmpl">
		
<!--<TMPL_VAR NAME="TITLE">-->

<form name="table" action="/doaboo-cgi/db.pl" method="post">
<center><table id="DataTable" class="data font">

 <!--Header line - Campos Loop-->
 <tr class="headerline">
   <!-- Control cells -->
   <td class="control">
     <a href="/doaboo-cgi/db.pl?end=<TMPL_VAR NAME="Initrecord">">Test-<TMPL_VAR NAME="Initrecord"></a>
   </td>
   <td class="control">&nbsp;</td>
   <td class="control">Icon</td>
   <!-- Column Names Loop -->
   <TMPL_LOOP NAME="Campos">
    <td><TMPL_VAR NAME="Nombre"></td>
   </TMPL_LOOP>
 </tr> 
 
 <!-- First line starts. Closed with </tr> inside the instances loop, when Newline detected -->
 <tr class="oddline" id="tr_<TMPL_VAR NAME="Initrecord">">
     <!-- Control Cells -->
     <td class="control">
       <input type="checkbox" name="check_<TMPL_VAR NAME="Initrecord">" onClick="if(this.checked == true){SelectInstance('<TMPL_VAR NAME="Initrecord">','1');} else {SelectInstance('<TMPL_VAR NAME="Initrecord">','0');}" value="<TMPL_VAR NAME="key">" <TMPL_IF NAME="selected">checked</TMPL_IF>>
     </td>
     <td class="control">
    	   Iconx2
	 </td>
     <td class="control">
       &nbsp;
	  </td>

 <!-- Instances Loop - Cell by Cell -->
 <TMPL_LOOP NAME="Valores">
	 <TMPL_IF NAME="Newline">
	  	</tr>
	    <TMPL_UNLESS NAME="__LAST__">
		<tr class="oddline" id="tr_<TMPL_VAR NAME="Index">">
	    <!-- Control Cells -->
         <td class="control">
		   <input type="checkbox" name="check_<TMPL_VAR NAME="Index">" onClick="if(this.checked == true){SelectInstance('<TMPL_VAR NAME="Index">','1');} else {SelectInstance('<TMPL_VAR NAME="Index">','0');} " value="<TMPL_VAR NAME="key">" <TMPL_IF NAME="selected">checked</TMPL_IF>>
	     </td>
         <td class="control">
       	   Iconx2
	     </td>
         <td class="control">
           &nbsp;
	     </td>
	    <!-- /Control Cells -->
        <TMPL_ELSE> <!-- Unless -->
           <!-- Last Line-->
           <tr class="headerline">
             <td class="control">
             <!--<input type="hidden" name="init" value="TMPL_VAR NAME="Index"">
 	         <img src="" onclick="javascript:document.table.submit();">-->
	         <a href="/doaboo-cgi/db.pl?init=<TMPL_VAR NAME="Index">">Test-<TMPL_VAR NAME="Index"></a>
	         <!--Not working yet-->
	         <!--<img src="/doaboo/images/boxdown.png" align="absmiddle" onClick="javascript:showResult('userchoice','Nextpage','external');">-->
             </td>
 	         <td class="control">&nbsp;</td>
   	         <td class="control">Icon</td>
   	         <!-- This last line is closed with </tr> outside the instances loop -->
	    </TMPL_UNLESS>
	 <TMPL_ELSE>  <!-- If -->
	    <td><TMPL_VAR NAME="Valor"></td>     	
	 </TMPL_IF>
	  	  	    
 </TMPL_LOOP>
   
     <!-- Continuation of the last table line: Column Names Loop -->
   	 <TMPL_LOOP NAME="Campos">
     <td><input type="text" value="<TMPL_VAR NAME="Nombre">"></td>
   	 </TMPL_LOOP>
   </tr>
   
</table></center></form>

<div>Showing results <TMPL_VAR name="Initrecord">-<TMPL_VAR name="Endrecord"> from a total of <TMPL_VAR name="INSTANCES_NUMBER"> in <TMPL_VAR name="COLUMNS_NUMBER"> columns</div>

</div> <!--ext-->
</body>
</html>
